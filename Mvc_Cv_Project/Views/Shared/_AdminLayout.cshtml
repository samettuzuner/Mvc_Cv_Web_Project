                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Yetenek/Index/">
                            <i class="nc-icon nc-grid-45"></i>
                            <p>Yeteneklerim</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Projelerim/Index/">
                            <i class="nc-icon nc-atom"></i>
                            <p>Projelerim</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/Sertifika/Index/">
                            <i class="nc-icon nc-notes"></i>
                            <p>Sertifikalarım</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/Deneyim/Index/">
                            <i class="nc-icon nc-puzzle-10"></i>
                            <p>Tecrübelerim</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/Admin/Index/">
                            <i class="nc-icon nc-lock-circle-open"></i>
                            <p>Şifremi Değiştir</p>
                        </a>
                    </li>
                    <li class="nav-item active active-pro">
                        <a class="nav-link active" href="/Login/LogOut/">
                            <i class="nc-icon nc-button-power"></i>
                            <p>Çıkış Yap</p>
                        </a>
                    </li>

                </ul>
            </div>
        </div>
        <div class="main-panel">
            
            <div class="content">
                <div class="container-fluid">
                    <div class="section">
                    </div>
                    <div> @RenderBody()</div>
                </div>
            </div>
            
        </div>
    </div>
