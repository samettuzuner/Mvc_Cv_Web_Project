@model Mvc_Cv_Project.Models.Entity.TblEgitimlerim
@{
    ViewBag.Title = "EgitimDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3> Eğitim Güncelleme Sayfası</h3>
<br />
<form method="post">
    @Html.Label("Üniversite")
    @Html.TextBoxFor(x => x.Universite, new { @class = "form-control"})
    <br />
    @Html.Label("Bölüm")
    @Html.TextBoxFor(x => x.Bolum, new { @class = "form-control" })
    <br />
    @Html.Label("Gano")
    @Html.TextBoxFor(x => x.GNO, new { @class = "form-control" })
    <br />
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>

